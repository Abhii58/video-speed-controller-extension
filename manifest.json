{
  "manifest_version": 3,
  "name": "Customizable Video Speed Controller",
  "version": "1.0",
  "description": "Increase video speed with customizable controls.",
  "permissions": ["storage", "activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "icons/icon.png"
  },
  "icons": {
    "128": "icons/icon.png"
  }
}
